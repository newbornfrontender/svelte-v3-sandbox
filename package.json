{
  "name": "svelte-v3-sandbox",
  "version": "1.0.0",
  "description": "Svelte v3 Sandbox",
  "repository": "https://github.com/newbornfrontender/svelte-v3-sandbox.git",
  "author": "Infant Frontender <newbornfrontender@gmail.com>",
  "license": "MIT",
  "scripts": {
    "pretty": "prettier --write **/*.{html,js,json,md}",
    "clean": "rm -rf public/*{css,js,map}",
    "serve": "serve public",
    "start": "rollup -c -w",
    "build": "yarn clean && rollup -c"
  },
  "devDependencies": {
    "prettier": "1.17.0",
    "rollup": "1.11.3",
    "rollup-plugin-node-resolve": "4.2.4",
    "rollup-plugin-svelte": "5.0.3",
    "serve": "11.0.0",
    "svelte": "3.4.4"
  }
}
